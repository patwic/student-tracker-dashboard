{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "max-len": 0,
    "react/prop-types": 0,
    "no-undef": 1,
    "comma-dangle": 0,
    "no-unused-vars": 1,
    "jsx-a11y/no-static-element-interactions": 0,
    "keyword-spacing": 0,
    "one-var": 0,
    "react/self-closing-comp": 0,
    "react/prefer-stateless-function": 1,
    "prefer-const": 1,
    "guard-for-in": 0,
    "react/no-unescaped-entities": 0,
    "consistent-return": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120
      }
    ]
  },
  "plugins": [
    "prettier"
  ]
}